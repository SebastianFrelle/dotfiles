package org.apache.olingo.sample.annotation.model;

import org.apache.olingo.odata2.api.annotation.edm.EdmEntitySet;
import org.apache.olingo.odata2.api.annotation.edm.EdmEntityType;
import org.apache.olingo.odata2.api.annotation.edm.EdmKey;
import org.apache.olingo.odata2.api.annotation.edm.EdmNavigationProperty;
import org.apache.olingo.odata2.api.annotation.edm.EdmProperty;

/**
 *
 */
@EdmEntityType(namespace = "MyFormula")
@EdmEntitySet(name = "Workers")
public class Worker {

  @EdmKey
  @EdmProperty
  private String name;
  @EdmNavigationProperty
  private OrganisationalUnit unit;

  public String getName() {
    return name;
  }

  public void setName(String name) {
    this.name = name;
  }

  public OrganisationalUnit getUnit() {
      return unit;
  }
  
  public void setUnit(OrganisationalUnit unit) {
      this.unit = unit;
  }

  @Override
  public String toString() {
    return "Worker{" + "name=" + name + '}';
  }

}