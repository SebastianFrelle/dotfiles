package org.apache.olingo.sample.annotation.model;

import org.apache.olingo.odata2.api.annotation.edm.EdmEntitySet;
import org.apache.olingo.odata2.api.annotation.edm.EdmEntityType;
import org.apache.olingo.odata2.api.annotation.edm.EdmKey;
import org.apache.olingo.odata2.api.annotation.edm.EdmProperty;
import org.apache.olingo.odata2.api.annotation.edm.EdmNavigationProperty.Multiplicity;
import java.util.List;
import java.util.ArrayList;
import org.apache.olingo.odata2.api.annotation.edm.EdmNavigationProperty;

/**
 *
 */
@EdmEntityType(namespace = "MyFormula")
@EdmEntitySet(name = "Areas")
public class Area {

  @EdmKey
  @EdmProperty
  private String name;

  @EdmNavigationProperty(name = "Units", toType = OrganisationalUnit.class, toMultiplicity = Multiplicity.MANY)
  private List<OrganisationalUnit> units = new ArrayList<OrganisationalUnit>();

  @EdmNavigationProperty
  private Project project;

  public String getName() {
    return name;
  }

  public void setName(String name) {
    this.name = name;
  }

  public List<OrganisationalUnit> getOrganisationalUnit() {
    return units;
  }

  public void setOrganisationalUnit(List<OrganisationalUnit> units) {
    this.units = units;
  }

  public Project getProject() {
    return project;
  }

  public void setProject(Project project) {
    this.project = project;
  }



  @Override
  public String toString() {
    return "Project{" + "street=" + name + '}';
  }

}